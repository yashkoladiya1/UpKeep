                var api = this.api();
                nb_cols = api.columns().nodes().length;
                var j = 6;
                while (j < nb_cols) {
                    if (j == 7 || j == 8 || j == 9 || j == 12 || j == 14 || j == 16 || j == 19 || j == 20) {
                        j++;
                    }
                    else {
                        var pageTotal = api
