                        ).toFixed(2);
                    }
                    else {
                        tmpCalcGrossAmt = (Number($("#txtQTY").val()) * Number($("#txtRate").val())).toFixed(2);
                        tmpCalcNetAmt = Number(
                            Number((Number($("#txtQTY").val()) * Number($("#txtRate").val())).toFixed(2))
                            -
