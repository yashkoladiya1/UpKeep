                            -
                            ((((Number($("#txtQTY").val()) * Number($("#txtRate").val())) - $("#txtDiscount").val()) * Number(IGST)) / 100)
                        ).toFixed(2);

                        tmpCalcNetAmt = Number(
                            (Number($("#txtQTY").val()) * Number($("#txtRate").val())).toFixed(2)
                            -
                            Number($("#txtDiscount").val())
                        ).toFixed(2);
