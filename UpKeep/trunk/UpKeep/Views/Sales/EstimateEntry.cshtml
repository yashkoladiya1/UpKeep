                        //alert(((((Number($("#txtQTY").val()) * Number($("#txtRate").val())) - $("#txtDiscount").val())) / 100));
                        tmpCalcGrossAmt = ((Number($("#txtQTY").val()) * Number($("#txtRate").val()))
                            - Number($("#txtDiscount").val())
                        ).toFixed(2);

                        tmpCalcNetAmt = Number(
                            (Number($("#txtQTY").val()) * Number($("#txtRate").val())).toFixed(2)
                            -
